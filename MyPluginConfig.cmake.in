# CMake package configuration file for the @PROJET_NAME@ plugin

@PACKAGE_INIT@
set(MYPLUGIN_HAVE_SOFAPYTHON @MYPLUGIN_HAVE_SOFAPYTHON@)
set(MYPLUGIN_HAVE_SOFAPYTHON3 @MYPLUGIN_HAVE_SOFAPYTHON3@)
set(MYPLUGIN_HAVE_STLIB @MYPLUGIN_HAVE_STLIB@)
set(MYPLUGIN_HAVE_SOFA_GL @MYPLUGIN_HAVE_SOFA_GL@)

find_package(Sofa.Config QUIET REQUIRED)
find_package(Sofa.Core QUIET REQUIRED)

find_package(Qt6 QUIET REQUIRED COMPONENTS Network)

if(MYPLUGIN_HAVE_SOFAPYTHON)
    find_package(SofaPython QUIET REQUIRED)
endif()

if(MYPLUGIN_HAVE_SOFAPYTHON3)
    find_package(SofaPython3 QUIET REQUIRED)
endif()

if(NOT TARGET @PROJECT_NAME@)
    include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")
endif()

check_required_components(@PROJECT_NAME@)